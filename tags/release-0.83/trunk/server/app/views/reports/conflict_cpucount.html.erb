<%- @tooltips = [] %>
<%- @page_title = 'Nodes which' + tooltip(Node,:os_processor_count,'OS CPU Count') + ' != ' +  tooltip(Node,:processor_count,'Physical CPU Count') %>
<h1>Nodes which <%= tooltip(Node,:os_processor_count,'OS CPU Count') %> != <%= tooltip(Node,:processor_count,'Physical CPU Count') %></h1>

<table width="100%">
<tr>
<td>
<%= page_entries_info @objects %>
</td>
</tr>
</table>

<div id="results_table">
<table cellspacing="0" class="styled">
<thead>
<%= will_paginate @objects %>
<tr>
  <th nowrap="nowrap" width="5%">&nbsp;</th>
  <th <%= sort_td_class_helper "name" %> > <%= sort_link_helper "Name", "name" %></th>
  <th>os cpu count</th>
  <th>physical cpu count</th>
  <th <%= sort_td_class_helper "updated_at" %> > <%= sort_link_helper "Updated At", "updated_at" %></th>
</tr>
</thead>
<tbody>
<%= render :partial => 'conflict_cpucount_row', :collection => @objects %>
</tbody>
</table>
</div> <!-- end results_table div -->

<%= @tooltips %>
