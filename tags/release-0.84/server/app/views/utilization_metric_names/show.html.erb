<%- @tooltips = [] %>
<%- @page_title = "Utilization Metric Name: " + h(@utilization_metric_name.name) %>

<div class="record_view">

<h1>Utilization Metric Name: <%=h @utilization_metric_name.name %></h1>

<p class="metadata"><strong>Created at:</strong> <%= h(@utilization_metric_name.created_at) %><br />
<strong>Updated at:</strong> <%= h(@utilization_metric_name.updated_at) %></p>

</div>

<div class="relationship">

<%= render :partial => 'shared/comments', :locals => { :object => @utilization_metric_name} %>

</div> <!-- end relationship -->

<p><%= link_to 'Edit', edit_utilization_metric_name_path(@utilization_metric_name) %> | 
<%= link_to 'Delete', utilization_metric_name_path(@utilization_metric_name), :confirm => 'Are you sure?', :method => :delete %></p>

<%= render :partial => 'shared/version_information', :locals => { :object => @utilization_metric_name} %>

<%= @tooltips %>
