<%- unless @tooltips ; then @tooltips = []; ttflag = true ; else; ttflag = false ;  end %>
<tr class="<%= cycle('odd', 'even') -%>">
  <td nowrap="nowrap" width="5%"><%= link_to 'View', self.send(index_row.class.to_s.underscore + '_path', index_row) %> 
<%- if allow_perm(index_row, ['updater']) %>
 / <%= link_to 'Edit', self.send('edit_' + index_row.class.to_s.underscore + '_path', index_row) %>
<%- end -%></td>
  <td><%= link_to index_row.ip_address.address, ip_address_path(index_row.ip_address) %></td>
  <td><%= link_to "#{index_row.network_port.number}/#{index_row.network_port.protocol}", network_port_path(index_row.network_port) %></td>
  <td><%= index_row.apps %></td>
  <td><%= index_row.assigned_at.to_formatted_s(:long) %></td>
  <td><%= index_row.nmap_first_seen_at.to_formatted_s(:long) if index_row.nmap_first_seen_at %></td>
  <td><%= index_row.nmap_last_seen_at.to_formatted_s(:long) if index_row.nmap_last_seen_at %></td>
</tr>

<%= @tooltips if ttflag %>
