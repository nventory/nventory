<strong><%= tooltip(NodeGroupNodeGroupAssignment,:updated_at) %>:</strong> <%= h(@node_group_node_group_assignment.updated_at) %></p>

</div>

<p><%= link_to 'Edit', edit_node_group_node_group_assignment_path(@node_group_node_group_assignment) %> | 
<%= link_to 'Delete', node_group_node_group_assignment_path(@node_group_node_group_assignment), :confirm => 'Are you sure?', :method => :delete %></p>

<%= render :partial => 'shared/version_information', :locals => { :object => @node_group_node_group_assignment} %>

