<tr class="<%= cycle('odd', 'even') -%>">
  <td nowrap="nowrap" width="5%"><%= link_to 'View', self.send(index_row.class.to_s.underscore + '_path', index_row) %> / <%= link_to 'Edit', self.send('edit_' + index_row.class.to_s.underscore + '_path', index_row) %>
  <td><%= link_to index_row.lb_pool.name, lb_pool_path(index_row.lb_pool) %></td>
  <td><%= link_to index_row.vip.name, vip_path(index_row.vip) %></td>
  <td><%= link_to index_row.assigned_at.to_formatted_s(:long), vip_lb_pool_assignment_path(index_row) %></td>
</tr>
